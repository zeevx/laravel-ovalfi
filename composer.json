{
    "name": "zeevx/laravel-ovalfi",
    "description": "A Laravel Package for OvalFi - https://dev.ovalfi-app.com/",
    "keywords": [
        "zeevx",
        "laravel-ovalfi"
    ],
    "homepage": "https://github.com/zeevx/laravel-ovalfi",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Paul Adams",
            "email": "adamsohiani@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zeevx\\LaravelOvalfi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zeevx\\LaravelOvalfi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zeevx\\LaravelOvalfi\\LaravelOvalfiServiceProvider"
            ],
            "aliases": {
                "LaravelOvalfi": "Zeevx\\LaravelOvalfi\\LaravelOvalfiFacade"
            }
        }
    }
}
